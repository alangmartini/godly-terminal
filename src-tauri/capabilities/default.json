{
  "$schema": "https://schema.tauri.app/config/2.0.0/capability",
  "identifier": "default",
  "description": "Default capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:window:allow-request-user-attention",
    "store:default",
    "dialog:default",
    "opener:default",
    "opener:allow-open-path",
    "notification:default"
  ]
}
